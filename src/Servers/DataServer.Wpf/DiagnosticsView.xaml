    </UserControl.Resources>

    <UserControl.Background>
        <VisualBrush Stretch="Fill" Visual="{StaticResource RxdSolutionsBackground}" Opacity="1" />
    </UserControl.Background>

    <Grid>

        <Grid.ColumnDefinitions>
            <ColumnDefinition />
            <ColumnDefinition />
            <ColumnDefinition />
            <ColumnDefinition />
        </Grid.ColumnDefinitions>

        <Grid.RowDefinitions>
            <RowDefinition Height="50"/>
            <RowDefinition Height="100"/>
            <RowDefinition Height="50"/>
            <RowDefinition Height="50" />
            <RowDefinition Height="50" />
            <RowDefinition Height="50" />
            <RowDefinition Height="50" />
            <RowDefinition Height="50" />
            <RowDefinition Height="50" />
            <RowDefinition Height="50" />
        </Grid.RowDefinitions>

        <TextBlock Foreground="Black" FontSize="25" Margin="10" HorizontalAlignment="Left" VerticalAlignment="Center" Cursor="Hand" Grid.ColumnSpan="4" MouseDown="Logo_MouseDown">FusionLink</TextBlock>

        <Grid Grid.Column="3" Grid.Row="0" Height="50" Width="50" Margin="0,0,10,0" HorizontalAlignment="Right" Cursor="Hand" MouseDown="Logo_MouseDown">
            <Grid.Background>
                <VisualBrush Stretch="Fill" Visual="{StaticResource RxdSolutionsLogo}" />
            </Grid.Background>
        </Grid>

        <Border Margin="10" Grid.Column="0" Grid.Row="1" Grid.RowSpan="2" BorderBrush="Black" Background="WhiteSmoke" BorderThickness="1" />
        <Border Margin="10" Grid.Column="1" Grid.Row="1" Grid.RowSpan="2" BorderBrush="Black" Background="WhiteSmoke" BorderThickness="1" />
        <Border Margin="10" Grid.Column="2" Grid.Row="1" Grid.RowSpan="2" BorderBrush="Black" Background="WhiteSmoke" BorderThickness="1" />
        <Border Margin="10" Grid.Column="3" Grid.Row="1" Grid.RowSpan="2" BorderBrush="Black" Background="WhiteSmoke" BorderThickness="1" />

        <TextBlock Grid.Column="0" Grid.Row="1" Text="{Binding PortfolioSubscriptionCount}" Style="{StaticResource SubscriptionCount}"></TextBlock>
        <TextBlock Grid.Column="1" Grid.Row="1" Text="{Binding PositionSubscriptionCount}" Style="{StaticResource SubscriptionCount}"></TextBlock>
        <TextBlock Grid.Column="2" Grid.Row="1" Text="{Binding SystemSubscriptionCount}" Style="{StaticResource SubscriptionCount}"></TextBlock>
        <TextBlock Grid.Column="3" Grid.Row="1" Text="{Binding PortfolioPropertySubscriptionCount}" Style="{StaticResource SubscriptionCount}"></TextBlock>

        <TextBlock Grid.Column="0" Grid.Row="2" Text="Portfolio Subscriptions" Style="{StaticResource SubscriptionLabel}" />
        <TextBlock Grid.Column="1" Grid.Row="2" Text="Position Subscriptions" Style="{StaticResource SubscriptionLabel}"  />
        <TextBlock Grid.Column="2" Grid.Row="2" Text="System Subscriptions" Style="{StaticResource SubscriptionLabel}" />
        <TextBlock Grid.Column="3" Grid.Row="2" Text="Portfolio Property Subscriptions" Style="{StaticResource SubscriptionLabel}" />

        <TextBlock Grid.Row="4" Grid.Column="0" Style="{StaticResource FieldLabel}" >Clients connected:</TextBlock>
        <TextBlock Grid.Row="4" Grid.Column="1" Style="{StaticResource FieldValue}" Text="{Binding ClientCount}"  />

        <TextBlock Grid.Row="5" Grid.Column="0" Style="{StaticResource FieldLabel}" >Listening on:</TextBlock>
        <TextBlock Grid.Row="5" Grid.Column="1" Style="{StaticResource FieldValue}" Text="{Binding ServerUri}" />

        <TextBlock Grid.Row="6" Grid.Column="0" Style="{StaticResource FieldLabel}" >Terminal Services:</TextBlock>
        <TextBlock Grid.Row="6" Grid.Column="1" Style="{StaticResource FieldValue}" Text="{Binding IsTerminalServices}"  />

        <TextBlock Grid.Row="7" Grid.Column="0" Style="{StaticResource FieldLabel}" >Last refresh duration:</TextBlock>
        <TextBlock Grid.Row="7" Grid.Column="1" Style="{StaticResource FieldValue}" Text="{Binding LastTimeTaken}" />
        <!-- , StringFormat=ss\.fff -->

        <TextBlock Grid.Row="7" Grid.Column="2" Style="{StaticResource FieldLabel}" >Average refresh duration:</TextBlock>
        <TextBlock Grid.Row="7" Grid.Column="3" Style="{StaticResource FieldValue}" Text="{Binding AverageTimeTaken}"  />
        <!-- ,StringFormat=ss\.fff-->

        <TextBlock Grid.Row="8" Grid.Column="0" Style="{StaticResource FieldLabel}" >Data server running:</TextBlock>
        <TextBlock Grid.Row="8" Grid.Column="1" Grid.ColumnSpan="1" Style="{StaticResource FieldValue}" Text="{Binding IsRunning}" />

        <Button Grid.Row="8" Grid.Column="2" Margin="10" Command="{Binding ToggleDataServer}" Content="{Binding ToggleDataServerLabel}"></Button>

        <TextBlock Grid.Row="9" Grid.Column="0" Style="{StaticResource FieldLabel}" >Publish queue:</TextBlock>
        <TextBlock Grid.Row="9" Grid.Column="1" Style="{StaticResource FieldValue}" Text="{Binding PublishQueueCount}"/>

    </Grid>

</UserControl>
